void HPL_rollN
(
   HPL_T_panel * PBCST,
   int * IFLAG,
   HPL_T_panel * PANEL,
   const int N,
   double * U,
   const int LDU,
   const int * IPLEN,
   const int * IPMAP,
   const int * IPMAPM1
)
# 81 "../HPL_rollN.c"
{
# 140 "../HPL_rollN.c"
   MPI_Datatype type[2];
   MPI_Status status;
   MPI_Request request;
   MPI_Comm comm;
   int Cmsgid=1001, ibufR, ibufS,
                              ierr=0, il, k, l, lengthR,
                              lengthS, mydist, myrow, next, npm1, nprow,
                              partner, prev;



   if( N <= 0 ) return;

   npm1 = ( nprow = PANEL->grid->nprow ) - 1; myrow = PANEL->grid->myrow;
   comm = PANEL->grid->col_comm;



   mydist = IPMAPM1[myrow];
   prev = IPMAP[( ((mydist)!=0) ? (mydist)-1 : (nprow)-1 )];
   next = IPMAP[( ((mydist) != (nprow)-1) ? (mydist) + 1 : 0 )];

   for( k = 0; k < npm1; k++ )
   {
      l = (int)( (unsigned int)(k) >> 1 );

      if( ( ( mydist + k ) & 1 ) != 0 )
      {
         il = ( ( (mydist) + (l) < (nprow) ) ? (mydist) + (l) : (mydist) + (l) - (nprow) );
         lengthS = IPLEN[il+1] - ( ibufS = IPLEN[il] );
         il = ( ( (mydist) < (l+1) ) ? (nprow) + (mydist) - (l+1) : (mydist) - (l+1) );
         lengthR = IPLEN[il+1] - ( ibufR = IPLEN[il] ); partner = prev;
      }
      else
      {
         il = ( ( (mydist) < (l) ) ? (nprow) + (mydist) - (l) : (mydist) - (l) );
         lengthS = IPLEN[il+1] - ( ibufS = IPLEN[il] );
         il = ( ( (mydist) + (l+1) < (nprow) ) ? (mydist) + (l+1) : (mydist) + (l+1) - (nprow) );
         lengthR = IPLEN[il+1] - ( ibufR = IPLEN[il] ); partner = next;
      }

      if( lengthR > 0 )
      {
         if( ierr == 0 )
            ierr = MPI_Type_vector( N, lengthR, LDU, ((MPI_Datatype)0x4c00080b),
                                      &type[1] );
         if( ierr == 0 )
            ierr = MPI_Type_commit( &type[1] );
         if( ierr == 0 )
            ierr = MPI_Irecv( ( (U) + (ibufR) + (0)*(LDU) ), 1, type[1],
                                partner, Cmsgid, comm, &request );
      }

      if( lengthS > 0 )
      {
         if( ierr == 0 )
            ierr = MPI_Type_vector( N, lengthS, LDU, ((MPI_Datatype)0x4c00080b),
                                      &type[0] );
         if( ierr == 0 )
            ierr = MPI_Type_commit( &type[0] );
         if( ierr == 0 )
            ierr = MPI_Send( ( (U) + (ibufS) + (0)*(LDU) ), 1, type[0],
                               partner, Cmsgid, comm );
         if( ierr == 0 )
            ierr = MPI_Type_free( &type[0] );
      }

      if( lengthR > 0 )
      {
         if( ierr == 0 )
            ierr = MPI_Wait( &request, &status );
         if( ierr == 0 )
            ierr = MPI_Type_free( &type[1] );
      }



      if( *IFLAG == 2 ) (void) HPL_bcast( PBCST, IFLAG );
   }

   if( ierr != 0 )
   { HPL_pabort( 221, "HPL_rollN", "MPI call failed" ); }



}
